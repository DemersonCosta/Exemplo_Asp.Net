<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Administrativo</title>

    @if (IsSectionDefined("Styles"))
    {@RenderSection("Styles", required: false)}

    @Styles.Render("~/Content/AdminCss")


    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>*@

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body ng-app="winterfell">
    <div id="wrapper">
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Admin Panel</a>
            </div>
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul id="active" class="nav navbar-nav side-nav">
                    <li><a href="@Url.Action("Index","Administrativo")"><i class="fa fa-bullseye" style="padding-right:5px"></i>Dashboard</a></li>
                    <li><a href="@Url.Action("Index","Usuario")"><i class="fa fa-user" style="padding-right:5px"></i>Usuario</a></li>
                    <li><a href="#"><i></i> Blog</a></li>
                    <li><a href="#"><i></i> SignUp</a></li>
                    <li><a href="#"><i></i> Register</a></li>
                    <li><a href="#"><i></i> Timeline</a></li>
                    <li><a href="#"><i></i> Forms</a></li>
                    <li><a href="#"><i></i> Typography</a></li>
                </ul>
            </div>
        </nav>
        <div class="container">
            @RenderBody()
        </div>


    </div>
    <!-- /#wrapper -->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/angular")
    @RenderSection("scripts", required: false)

</body>
</html>
